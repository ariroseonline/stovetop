{
  "name": "Stovetop",
  "version": "0.0.2",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Open Stovetop",
    "default_popup": "index.html"
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}